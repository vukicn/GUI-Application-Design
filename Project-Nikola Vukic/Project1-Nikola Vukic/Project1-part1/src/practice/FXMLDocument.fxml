<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="practice.FXMLDocumentController">
    <children>
      <HBox layoutY="6.0" prefHeight="65.0" prefWidth="320.0">
         <children>
              <Button fx:id="button" onAction="#handleButtonAction" prefHeight="25.0" prefWidth="130.0" text="Start here!" />
              <Label fx:id="label" lineSpacing="10.0" minHeight="16" minWidth="69" prefHeight="25.0" prefWidth="171.0" />
         </children>
      </HBox>
      <VBox layoutX="-1.0" layoutY="39.0" prefHeight="200.0" prefWidth="107.0">
         <children>
            <TextField fx:id="textfield" prefHeight="25.0" prefWidth="39.0" />
            <Label prefHeight="25.0" prefWidth="111.0" text="${textfield.text}" />
         </children>
      </VBox>
    </children>
</AnchorPane>
